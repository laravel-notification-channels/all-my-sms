{
    "name": "laravel-notification-channels/all-my-sms",
    "description": "AllMySms Notifications driver for Laravel",
    "homepage": "https://github.com/laravel-notification-channels/all-my-sms",
    "license": "MIT",
    "authors": [
        {
            "name": "MikaÃ«l Popowicz",
            "email": "mikael.popowicz@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "guzzlehttp/guzzle": "^6.2 || ^7.0",
        "illuminate/notifications": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0",
        "illuminate/support": "^5.5 || ^6.0 || ^7.0 || ^8.0 || ^9.0 || ^10.0 || ^11.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.6.7",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "NotificationChannels\\AllMySms\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationChannels\\AllMySms\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --no-coverage",
        "test:coverage": "XDEBUG_MODE=coverage vendor/bin/phpunit --coverage-text"
    },
    "config": {
        "sort-packages": true
    }
}
